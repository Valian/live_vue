{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declarationDir": "./priv/static",
    "declaration": true,
    "emitDeclarationOnly": true
  },
  "exclude": ["node_modules"]
}