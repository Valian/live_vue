{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declarationDir": "./priv/static",
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "./priv/static",
    "allowJs": true
  },
  "exclude": ["node_modules"],
}